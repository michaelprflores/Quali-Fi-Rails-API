<%= form_for @applicant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :skills %>
  <%= f.text_field :skills%>

  <%= f.label :education %>
  <%= f.text_field :education %>

  <%= f.label :years_experience %>
  <%= f.number_field :years_exp %>

  <%= f.label :local %>
  <%= f.text_field :local %>

  <%= f.submit %>
<% end %>